{
  "version": 2,
  "name": "areliverpoolwinning",
  "alias": "www.areliverpoolwinning.com",
  "regions": ["bru", "bru1"],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next",
      "config": { "maxLambdaSize": "20mb" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "status": 301, "headers": { "Location": "https://areliverpoolwinning.com/$1" }}
 ],
  "env": {
    "GA_KEY": "@ga_key"
  }
}
